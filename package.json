{
  "name": "things-scene-3d-viewer",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile": "./node_modules/.bin/babel --presets es2015 -d lib/ src/",
    "prepublish": "npm run compile",
    "test": "./node_modules/.bin/mocha --require should --compilers js:babel-core/register test/**/test-*.js",
    "pretest": "npm run compile",
    "watchify": "watchify --extension=js -o things-scene-3d-viewer.js index.js",
    "serve": "watchify --extension=js -o things-scene-3d-viewer.js index.js & python -m SimpleHTTPServer",
    "build": "browserify index.js | uglifyjs -c > things-scene-3d-viewer-min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThingsElements/things-scene-3d-viewer.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ThingsElements/things-scene-3d-viewer/issues"
  },
  "homepage": "https://github.com/ThingsElements/things-scene-3d-viewer#readme",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "~7.2.0",
    "browserify": "~10.2.4",
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "should": "^8.0.2",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.3.1"
  },
  "dependencies": {
    "things-scene": "^0.4.15"
  }
}
